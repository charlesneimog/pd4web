-- ──────────────────────────────────────────
function vu:rms_to_linear(rms_db)
	local min_db, max_db = -100, 12
	if rms_db < min_db then
		rms_db = min_db
	end
	if rms_db > max_db then
		rms_db = max_db
	end
	return (rms_db - min_db) / (max_db - min_db)
end

