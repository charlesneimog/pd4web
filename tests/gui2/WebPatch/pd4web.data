-- ─────────────────────────────────────
function listatom:initialize(name, args)
	self.inlets = 1
	self.outlets = 1
	self.args = args
	self.width = 20
	self.labelpos = 4
	self.idontknow = 0
	self.label = "-"
	self.receive = "-"
	self.send = "-"
